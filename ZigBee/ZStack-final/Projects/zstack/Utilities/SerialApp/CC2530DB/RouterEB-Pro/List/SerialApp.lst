    233              //sprintf(buff,"send_uart, %p %d.\n\r",buffer,size);
    234              //HalUARTWrite( SERIAL_APP_PORT, (uint8*)buff, strlen(buff));
    235              HalUARTWrite( SERIAL_APP_PORT, buffer, size);
    236            }
    237            else{
    238              return;
    239            }
    240          }
